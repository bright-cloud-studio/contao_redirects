{
    "name":"bright-cloud-studio/contao_redirects",
    "description":"URL redirection module for Contao 4.9+",
    "homepage": "https://andrewstevens.consulting",
    "keywords":["contao", "htaccess", ".htaccess", "redirect", "rewrite", "url", "301", "302", "bright", "cloud", "studio"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Bright Cloud Studio",
            "homepage":"https://brightcloudstudio.com"
        }
    ],
    "require":{
        "php": ">=7.4",
        "contao/core-bundle": ">=4.13",
        "contao-community-alliance/composer-plugin": "~2.4 || ~3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\": [
                "system/modules/contao_redirects/library/Bcs"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/contao_redirects": "system/modules/contao_redirects"
            }
        }
    },
    "minimum-stability": "dev"
}
